[alias]
	chekcout = checkout
	co = checkout
	d = difftool
	f = fetch --all
	lg = log --graph --date=relative --pretty=tformat:'%Cred%h%Creset -%C(auto)%d%Creset %s %C(yellow)(%an %ad)%Creset'
[branch]
	autosetupmerge = true
	autosetuprebase = always
[core]
	whitespace = -trailing-space
[diff]
	renames = copy
	tool = vscode
[difftool "vscode"]
    cmd = code --wait --diff $LOCAL $REMOTE
[help]
	autocorrect = 1
[merge]
    tool = vscode
[mergetool "vscode"]
    cmd = code --wait $MERGED
[push]
	default = current
[rebase]
	abbreviateCommands = true
[rerere]
	autoUpdate = false
	enabled = false
[url "git@github.com:"]
	insteadOf = https://github.com/
