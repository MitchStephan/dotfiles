[alias]
	aliases = config --get-regexp alias
	amend = commit --amend --date=now
	ammend = commit --amend --date=now
	br = branch --format='%(HEAD) %(color:yellow)%(refname:short)%(color:reset) - %(contents:subject) %(color:green)(%(committerdate:relative)) [%(authorname)]' --sort=-committerdate
	c = clone --recursive
	chekcout = checkout
	co = checkout
	d = difftool
	delete-merged = "!git branch --merged | egrep -v '(^\\*|master|main)' | xargs git branch -d"
	delete-unmerged = "!git branch --no-merged | egrep -v '(^\\*|master|main)' | xargs git branch -D"
	f = fetch --tags --all
	lg = log --graph --date=relative --pretty=tformat:'%Cred%h%Creset -%C(auto)%d%Creset %s %C(yellow)(%an %ad)%Creset'
	p = pull --tags --recurse-submodules
	s = status
	undo = reset HEAD~1 --mixed
	unstage = reset HEAD
	up = push origin HEAD
[branch]
	autosetupmerge = true
	autosetuprebase = always
[core]
	editor = code --wait
	untrackedCache = true
	whitespace = -trailing-space
[diff]
	renames = copy
	tool = vscode
[difftool]
	prompt = false
[difftool "vscode"]
	cmd = code --wait --diff $LOCAL $REMOTE
[fetch]
	prune = true
	pruneTags = true
[help]
	autocorrect = 1
[include]
	path = ~/local.gitconfig
[merge]
	log = true
	tool = vscode
[mergetool "vscode"]
	cmd = code --wait $MERGED
	keepBackup = false
[pull]
	ff = only
[push]
	default = current
[rebase]
	abbreviateCommands = true
[rerere]
	autoUpdate = false
	enabled = false
[url "git@github.com:"]
	insteadOf = https://github.com/
